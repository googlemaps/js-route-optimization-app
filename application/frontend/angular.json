{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "fleetrouting-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/fleetrouting-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/config.json",
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles/main.scss",
              "node_modules/nouislider/distribute/nouislider.min.css"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "@babel/runtime/regenerator",
              "@turf/boolean-crosses",
              "@turf/boolean-within",
              "@turf/line-intersect",
              "geojson-rbush",
              "hammerjs",
              "lodash",
              "nouislider",
              "probe.gl/env",
              "jspdf-autotable",
              "core-js",
              "earcut",
              "@mapbox/tiny-sdf",
              "raf",
              "rgbcolor",
              "html2canvas",
              "papaparse",
              "jszip"
            ]
          },
          "configurations": {
            "dev": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "input": "src/config/dev",
                  "output": "/",
                  "glob": "config.json"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "100kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "fleetrouting-app:build"
          },
          "configurations": {
            "dev": {
              "buildTarget": "fleetrouting-app:build:dev"
            },
            "production": {
              "buildTarget": "fleetrouting-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "fleetrouting-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverageExclude": [
              "**/*.spec.ts",
              "**/*.js",
              ".*/**/*",
              "dist/**/*",
              "e2e/**/*",
              "node_modules/**/*"
            ],
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/config.json",
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles/main.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "fleetrouting-app:serve"
          },
          "configurations": {
            "dev": {
              "devServerTarget": "fleetrouting-app:serve:dev"
            },
            "production": {
              "devServerTarget": "fleetrouting-app:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "4719daa3-cef3-48df-9165-97bc569220b4",
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
